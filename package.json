{
  "name": "what-did-i-do",
  "version": "1.0.0",
  "author": "Nick Alteen <ncalteen@github.com>",
  "description": "A GitHub Action to generate a summary of your GitHub activity.",
  "private": true,
  "type": "module",
  "homepage": "https://github.com/ncalteen/what-did-i-do#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ncalteen/what-did-i-do.git"
  },
  "bugs": {
    "url": "https://github.com/ncalteen/what-did-i-do/issues"
  },
  "engines": {
    "node": ">=20"
  },
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "bundle": "npm run format:write && npm run package",
    "ci-test": "npx jest --ci --reporters=default --reporters=jest-junit",
    "coverage": "npx make-coverage-badge --output-path ./badges/coverage.svg",
    "format:write": "npx prettier --write .",
    "format:check": "npx prettier --check .",
    "lint": "npx eslint .",
    "package": "npx ncc build src/index.ts -o dist --source-map --license licenses.txt",
    "package:watch": "npm run package -- --watch",
    "test": "npx jest",
    "all": "npm run format:write && npm run lint && npm run test && npm run coverage && npm run package"
  },
  "jest-junit": {
    "outputDirectory": "reports",
    "outputName": "jest-junit.xml"
  },
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^6.0.0",
    "@actions/glob": "^0.4.0",
    "@actions/io": "^1.1.3",
    "@octokit/plugin-rest-endpoint-methods": "^13.2.4",
    "@octokit/plugin-retry": "^7.1.1",
    "@octokit/plugin-throttling": "^9.3.0",
    "@octokit/rest": "^21.0.0",
    "ajv": "^8.14.0",
    "jsonc": "^2.0.0",
    "mustache": "^4.2.0",
    "ts-dedent": "^2.2.0",
    "yaml": "^2.4.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.24.6",
    "@octokit/graphql": "^8.1.1",
    "@octokit/graphql-schema": "^15.23.0",
    "@octokit/openapi-types": "^22.2.0",
    "@octokit/types": "^13.4.1",
    "@octokit/webhooks-types": "^7.5.1",
    "@types/jest": "^29.5.12",
    "@types/mustache": "^4.2.5",
    "@types/node": "^20.14.9",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-github": "^5.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.5.0",
    "eslint-plugin-jsonc": "^2.16.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "js-yaml": "^4.1.0",
    "make-coverage-badge": "^1.2.0",
    "prettier": "^3.3.2",
    "prettier-eslint": "^16.3.0",
    "ts-jest": "^29.1.5",
    "ts-jest-resolver": "^2.0.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.3"
  }
}
