{
  "name": "what-did-i-do",
  "version": "2.1.0",
  "author": "Nick Alteen <ncalteen@github.com>",
  "description": "A GitHub Action to generate a summary of your GitHub activity.",
  "private": true,
  "type": "module",
  "homepage": "https://github.com/ncalteen/what-did-i-do#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ncalteen/what-did-i-do.git"
  },
  "bugs": {
    "url": "https://github.com/ncalteen/what-did-i-do/issues"
  },
  "engines": {
    "node": ">=21"
  },
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "bundle": "npm run format:write && npm run package",
    "ci-test": "NODE_OPTIONS=--experimental-vm-modules NODE_NO_WARNINGS=1 npx jest",
    "coverage": "npx make-coverage-badge --output-path ./badges/coverage.svg",
    "format:write": "npx prettier --write .",
    "format:check": "npx prettier --check .",
    "lint": "npx eslint .",
    "package": "npx rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript",
    "test": "NODE_OPTIONS=--experimental-vm-modules NODE_NO_WARNINGS=1 npx jest",
    "all": "npm run format:write && npm run lint && npm run test && npm run coverage && npm run package"
  },
  "jest-junit": {
    "outputDirectory": "reports",
    "outputName": "jest-junit.xml"
  },
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^6.0.0",
    "@actions/glob": "^0.5.0",
    "@actions/io": "^1.1.3",
    "@octokit/plugin-rest-endpoint-methods": "^13.2.4",
    "@octokit/plugin-retry": "^7.1.1",
    "@octokit/plugin-throttling": "^9.3.1",
    "@octokit/rest": "^21.0.2",
    "ajv": "^8.17.1",
    "jsonc": "^2.0.0",
    "mustache": "^4.2.0",
    "ts-dedent": "^2.2.0",
    "yaml": "^2.5.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.25.1",
    "@octokit/graphql": "^8.1.1",
    "@octokit/graphql-schema": "^15.25.0",
    "@octokit/openapi-types": "^22.2.0",
    "@octokit/types": "^13.4.1",
    "@octokit/webhooks-types": "^7.5.1",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/jest": "^29.5.12",
    "@types/mustache": "^4.2.5",
    "@types/node": "^22.5.2",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@typescript-eslint/parser": "^8.4.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.8.1",
    "eslint-plugin-jsonc": "^2.16.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "js-yaml": "^4.1.0",
    "make-coverage-badge": "^1.2.0",
    "prettier": "^3.3.3",
    "prettier-eslint": "^16.3.0",
    "rollup": "^4.21.2",
    "ts-jest": "^29.2.5",
    "ts-jest-resolver": "^2.0.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4"
  }
}
